{
  "private": true,
  "author": {
    "name": "hexiao-o",
    "email": "460559761@qq.com"
  },
  "build": {
    "files": [
      "./dist/**/*",
      "./pages/**/*",
      "./node_modules/**/*"
    ],
    "appId": "com.hexiao-o.yunle-template-electron",
    "asar": false
  },
  "scripts": {
    "start": "electron app/dist/main/main",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:main": "ESLINT=none BROWSER=none COMPRESS=none roadhog build -w",
    "dev:renderer": "cd src/renderer && BROWSER=none ESLINT=none umi dev",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "ESLINT=none roadhog build",
    "build:renderer": "cd src/renderer && ESLINT=none umi build",
    "pack": "npm run build && npm run rebuild && build",
    "pack:dir": "npm run build && npm run rebuild && build --dir",
    "pack:dirOnly": "build --dir",
    "rebuild": "electron-rebuild -m ./app"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^3.0.4",
    "electron-builder": "^20.28.4",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.2",
    "roadhog": "^2.4.5",
    "slash": "^2.0.0",
    "umi": "^2.1.2",
    "umi-plugin-react": "^1.1.1"
  },
  "dependencies": {}
}
