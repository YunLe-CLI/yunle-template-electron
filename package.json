{
  "productName": "YunLe.AI",
  "author": "YunLe.AI",
  "name": "YunLe.AI",
  "description": "YunLe.AI",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "postinstall": "",
    "start": "concurrently \"npm run start:dva\" \"electron --inspect=5858 . --DESKTOP_APP_ENV=develop\"",
    "start:pro": "concurrently \"npm run build:dva\" \"electron --inspect=5858 . --DESKTOP_APP_ENV=production\"",
    "start:dva": "cd src && npm run develop",
    "build:dva": "cd src && npm run build",
    "build:win-32": "node ./electron-builder.js && npm run build:dva && cross-env version=$npm_package_version build -w --ia32 --publish=never --config ./electron-builder.yaml",
    "build:win-64": "node ./electron-builder.js && npm run build:dva && cross-env version=$npm_package_version build -w --x64 --publish=never --config ./electron-builder.yaml",
    "build:mac": "node ./electron-builder.js && npm run build:dva && cross-env version=$npm_package_version cross-env CSC_NAME='xiao he (U2784YTGC6)' build -m --publish=never --config ./electron-builder.yaml",
    "build": "npm run build:win-64 && npm run build:mac"
  },
  "engines": {
    "install-node": "6.9.2"
  },
  "dependencies": {
    "ali-oss": "^6.0.1",
    "antd": "^3.7.2",
    "bluebird": "^3.5.1",
    "check-types": "^7.4.0",
    "co": "^4.6.0",
    "electron-builder-squirrel-windows": "^20.25.0",
    "electron-log": "^2.2.16",
    "electron-serve": "^0.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^3.0.3",
    "file-saver": "^1.3.8",
    "form-data": "^2.3.2",
    "formstream": "^1.1.0",
    "global": "^4.3.2",
    "lowdb": "^1.0.0",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "msr": "^1.3.4",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "redux-persist-electron-storage": "^1.1.2",
    "request": "^2.87.0",
    "request-progress": "^3.0.0",
    "request-promise": "^4.2.2",
    "speedtest-net": "^1.4.3",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "download": "^7.1.0",
    "electron": "^3.0.0-beta.3",
    "electron-builder": "^20.25.0",
    "electron-packager": "^12.1.0",
    "eslint": "^5.2.0",
    "pre-commit": "^1.2.2",
    "spectron": "^3.8.0",
    "typescript": "^2.9.2"
  }
}
